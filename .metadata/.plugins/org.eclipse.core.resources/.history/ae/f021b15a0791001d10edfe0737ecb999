package entities;

public class IndividualAccount extends Account{

	private Double HealthIncome;
	
	public IndividualAccount() {
		super();
	}

	public IndividualAccount(String name, Double anualIncome, Double healthIncome) {
		super(name, anualIncome);
		HealthIncome = healthIncome;
	}

	public Double getHealthIncome() {
		return HealthIncome;
	}

	public void setHealthIncome(Double healthIncome) {
		HealthIncome = healthIncome;
	}
	
	@Override
	public Double taxesPaid() {
		double taxesPaid = 0.0;
		if(super.getAnualIncome() < 20000) {
			taxesPaid = super.getAnualIncome() * 0.15 - HealthIncome * 0.50;
		}
		else {
			taxesPaid = super.getAnualIncome() * 0.25 - HealthIncome * 0.50;
		}
		return taxesPaid;
	}
}
